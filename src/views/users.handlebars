<h1>Usuarios</h1>

<table id="main" role="grid">
    <tr>
        <th>Nombre</th>
        <th>Apellido</th>
        <th>Correo</th>
        <th>Role</th>
    </tr>
    {{#each users}}
        <tr>
            <td>{{this.first_name}}</td>
            <td>{{this.lastName}}</td>
            <td>{{this.mail}}</td>
            <td>{{this.role}}</td>
        </tr>
    {{/each }}
</table>